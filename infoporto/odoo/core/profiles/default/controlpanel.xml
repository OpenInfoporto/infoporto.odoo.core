<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="infoporto.odoo.core"
    purge="False">

    <configlet
        title="Odoo configuration"
        action_id="odoo"
        appId="infoporto.odoo.core"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@odoo-settings"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>

